  <!-- Enable build. -->
  <Target Name="CoreCompile" />
  <ItemGroup>
    <Markdown Include="**\*.md" />
  </ItemGroup>
  <Target Name="GetMarkdownOutputItems" Returns="@(MarkdownOutput)">
    <RunPythonCommand Target="$(StartupFile)" TargetType="script" ExecuteIn="none" Arguments="$(CommandLineArgumentsPrefix) --convert --list-outputs-only" ConsoleToMSBuild="true">
      <Output TaskParameter="ConsoleOutput" PropertyName="_MarkdownOutputList" />
    </RunPythonCommand>
    <ItemGroup>
      <MarkdownOutput Include="$(_MarkdownOutputList.Split('&#xA;'))" />
    </ItemGroup>
  </Target>
  <!-- Disable output group based up-to-date checks, and rely on regular MSBuild logic. -->
  <Target Name="BuiltProjectOutputGroupFast_Markdown" Returns="@(BuiltProjectOutputGroupFastOutput)" AfterTargets="BuiltProjectOutputGroupFast" DependsOnTargets="GetMarkdownOutputItems">
    <ItemGroup>
      <BuiltProjectOutputGroupFastOutput Include="@(MarkdownOutput)" />
    </ItemGroup>
  </Target>
  <Target Name="ConvertMarkdownToHtml" AfterTargets="CoreCompile" DependsOnTargets="GetMarkdownOutputItems" Inputs="$(MSBuildProjectFile);@(Markdown)" Outputs="@(MarkdownOutput)">
    <RunPythonCommand Target="$(StartupFile)" TargetType="script" ExecuteIn="none" Arguments="$(CommandLineArgumentsPrefix) --convert">
    </RunPythonCommand>
    <ItemGroup>
      <FileWrites Include="@(MarkdownOutput)" />
    </ItemGroup>
  </Target>
